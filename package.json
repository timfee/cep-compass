{
  "name": "cep-compass",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "npm run test:unit",
    "test:unit": "ng test --watch=false --browsers=ChromeHeadlessCI",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "emulators:start": "npx firebase emulators:start",
    "lint": "ng lint",
    "format": "prettier --write ."
  },
  "prettier": {
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.1.3",
    "@angular/cdk": "^20.1.3",
    "@angular/common": "^20.1.3",
    "@angular/compiler": "^20.1.3",
    "@angular/core": "^20.1.3",
    "@angular/fire": "20.0.1",
    "@angular/forms": "^20.1.3",
    "@angular/material": "^20.1.3",
    "@angular/platform-browser": "^20.1.3",
    "@angular/router": "^20.1.3",
    "firebase": "12.0.0",
    "ngx-quill": "^28.0.1",
    "quill": "^2.0.3",
    "rxjs": "~7.8.2",
    "tslib": "^2.8.1",
    "zone.js": "~0.15.1"
  },
  "devDependencies": {
    "@angular-eslint/builder": "20.1.1",
    "@angular/build": "^20.1.3",
    "@angular/cli": "^20.1.3",
    "@angular/compiler-cli": "^20.1.3",
    "@eslint/js": "^9.32.0",
    "@playwright/test": "^1.54.1",
    "@types/jasmine": "~5.1.8",
    "@types/node": "24.1.0",
    "angular-eslint": "20.1.1",
    "concurrently": "^9.2.0",
    "eslint": "^9.32.0",
    "firebase-tools": "^14.11.1",
    "jasmine-core": "~5.9.0",
    "karma": "~6.4.4",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "prettier": "^3.6.2",
    "puppeteer": "^23.11.1",
    "typescript": "~5.8.3",
    "typescript-eslint": "8.38.0"
  }
}
